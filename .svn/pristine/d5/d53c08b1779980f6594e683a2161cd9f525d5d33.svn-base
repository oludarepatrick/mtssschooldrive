#menu{
list-style-type: none;
background:url(images/link_bac.gif) repeat-x;
height: 39px;
width: 663px;
margin: auto;
}
#menu li {
float: left;
background:url(images/divider.gif) no-repeat right;
display: block;
line-height: 39px;
}
/*#menu li.off ul, #nav li.on ul {
  /*hide the subnavs
  display: none;
   /*put the subnavs below and hide them all
  position: absolute;
  top: 133px;
  height: 35px;
  left: 0;
  padding-top: 10px;
}*/
#menu li a {

padding-right: 32px;
padding-left: 32px;
display: block;
line-height: 39px;
text-decoration: none;
font-family: Verdana, Geneva, sans-serif;
font-size: 13px;
color: #FFF;
}
#menu li a:hover, #menu li a.active{
color: #000;
background:url(images/sublink_1.gif) repeat-x;
height:39px;
}

/*Submenu*/
#menu li ul{ 
margin:0px;
padding:0px;
display:none;
color:#333;
position: absolute;
  top: 133px;
  height: 39px;
 padding-top: 2px;
}

/*-------------------------main link -#menu, sub menu-.sub-links */
.sub-links{
	margin:0px;
padding:0px;
display:none;
color:#333;
position: absolute;
  top: 133px;
  height: 39px;
 padding-top: 2px;
}

#menu li .sub-links li a:hover{
	color: #999;
background:none;
height:39px;
}
/*-------------------------*/
#menu li:hover ul{
	display:block;
}

#menu li li{
list-style:none;
display: block;
float:left;
background:none;
color:#333;
}
#menu li li a{
color:#333;
}


#menu li li a:hover {
color: #999;
background:none;
height:39px;
}
